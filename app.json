{
  "expo": {
    "name": "SpotifyMusicFuckinLyrics",
    "slug": "SpotifyMusicFuckinLyrics",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "SpotifyMusicFuckinLyrics",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-updates",
        {
          "username": "account-username"
        }
      ]],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
